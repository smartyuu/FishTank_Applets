}
.container .contain-box .title-box .input-box {
  margin: 2vw;
}
.container .contain-box .title-box .input-box .title {
  color: #edfffa;
  font-size: 54rpx;
  font-weight: bolder;
}
.container .contain-box .title-box .input-box .title-info {
  font-size: 32rpx;
  padding-top: 3vh;
  color: #99f9d9;
}
.container .contain-box .title-box .input-box .user, .container .contain-box .title-box .input-box .password {
  width: 60vw;
  height: 80rpx;
  margin-top: 8px;
  padding-left: 8px;
  background-color: white;
  border-radius: 10px;
}
.container .contain-box .title-box .introduction {
  color: #edfffa;
  font-size: 8vw;
  font-weight: bolder;
  margin: 2vw 1vw 1vw 2vw;
}
.container .contain-box .title-box .introduction span {
  display: flex;
}
.container .contain-box .title-box .introduction .intro {
  font-size: 7vw;
  color: #99f9d9;
}
.container .contain-box .title-box .text-pop-up-top {
  animation: text-pop-up-top 0.5s cubic-bezier(0.25, 0.46, 0.45, 0.94) both;
}
@keyframes text-pop-up-top {
0% {
    transform: translateY(0);
    transform-origin: 50% 50%;
    text-shadow: none;
}
50% {
    transform: translateY(-70rpx);
    transform-origin: 50% 50%;
    text-shadow: 0 1px 0 #293b34, 0 2px 0 #293b34, 0 3px 0 #293b34, 0 4px 0 #293b34, 0 5px 0 #293b34, 0 6px 0 #293b34, 0 7px 0 #293b34, 0 8px 0 #293b34, 0 9px 0 #293b34, 0 50px 30px rgba(0, 0, 0, 0.3);
}
100% {
    transform: translateY(-30rpx);
    transform-origin: 50% 50%;
    text-shadow: 0 1px 0 #293b34, 0 2px 0 #293b34, 0 3px 0 #293b34, 0 4px 0 #293b34, 0 5px 0 #293b34, 0 6px 0 #293b34, 0 7px 0 #293b34, 0 8px 0 #293b34, 0 9px 0 #293b34, 0 50px 30px rgba(0, 0, 0, 0.3);
}
}
.container .contain-box .title-box .tracking-in-expand {
  animation: tracking-in-expand 1.5s cubic-bezier(0.215, 0.61, 0.355, 1) both;
}
@keyframes tracking-in-expand {
0% {
    letter-spacing: -0.5em;
    opacity: 0;
}
40% {
    opacity: 0.6;
}
100% {
    opacity: 1;
}
}
.container .contain-box .title-box .button-box {
  display: flex;
}
.container .contain-box .title-box .button-box button {
  width: 25vw;
  border-radius: 30rpx;
}
.container .contain-box .title-box .button-box .login-button {
  color: #fff;
  background-color: #007aff;
}
.container .contain-box .title-box .wechat-tip {
  display: flex;
  justify-content: center;
  margin-top: 2vh;
  font-size: 1.5vh;
  font-weight: bold;
}
.container .contain-box .title-box .wechat {
  margin-top: 3vh;
  background-color: #008000;
  color: #fff;
  border-radius: 30px;
}